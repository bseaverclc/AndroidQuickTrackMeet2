<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidquicktrackmeet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Quick Track Meet"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.AndroidQuickTrackMeet" >
        <activity
            android:name="com.example.androidquicktrackmeet.SchoolsActivity"
            android:label="Schools"
            android:launchMode="singleTask"
            android:parentActivityName="com.example.androidquicktrackmeet.MainActivity" />
        <activity
            android:name="com.example.androidquicktrackmeet.MeetsActivity"
            android:label="Meets"
            android:launchMode="singleTask"
            android:parentActivityName="com.example.androidquicktrackmeet.MainActivity" />
        <activity android:name="com.example.androidquicktrackmeet.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
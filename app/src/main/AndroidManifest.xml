<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quickmeet.androidquicktrackmeet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Quick Track Meet"
        android:orientation="vertical"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidQuickTrackMeet">
        <activity android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.RelayActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.EventEditActivity"
            />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.schools.SchoolCoachesActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.schools.schoolAthletes" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.schools.ActivityAddSchool"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.schools.SchoolsActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.schools.schoolAthletes"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.schools.SchoolsActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.AddSchoolsToMeet"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.meetathletes.MeetAthletesActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.meetathletes.AthleteEventsFromMeets"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.AddMeetActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.meetathletes.AddEventsToAthlete"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.meetathletes.AthleteEventsFromMeets" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.meetathletes.MeetAthletesActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.HomeActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.addathletestoevent.AddAthlete"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.addathletestoevent.AddAthleteToEventActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.ScoresActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.HomeActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.addathletestoevent.AddAthleteToEventActivity"
            android:launchMode="singleTask"
            android:noHistory="true"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.EventEditActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.events.theevent.EventEditActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.events.EventsActivity"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.events.EventsActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.themeet.HomeActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.themeet.HomeActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.MeetsActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.AddMeetActivity"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.meets.MeetsActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.schools.SchoolsActivity"
            android:label="Schools"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.MainActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.meets.MeetsActivity"
            android:label="Meets"
            android:launchMode="singleTask"
            android:parentActivityName="com.quickmeet.androidquicktrackmeet.MainActivity" />
        <activity
            android:name="com.quickmeet.androidquicktrackmeet.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>